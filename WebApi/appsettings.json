{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Fatal",
      "Override": {
        "Microsoft.AspNetCore.Mvc": "Fatal",
        "System": "Fatal"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/app-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} || {Message:lj}{Exception}{NewLine}"
        }
      }
    ]
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "---"
  },

  "Jwt": {
    "Key": ",",
    "Issuer": "http://localhost:3033/",
    "Audience": "JWTServicePostmanClient"
  }
}
